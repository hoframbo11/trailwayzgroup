{
  "name": "trailwayz",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "client": "npm start --prefix client",
    "client-install": "cd client && npm install",
    "server": "cd server && nodemon server.js",
    "server-install": "cd server && npm install",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "hbs": "^4.0.4",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "requirejs-babel": "0.0.9"
  },
  "devDependencies": {
    "nodemon": "^1.19.1",
    "concurrently": "^3.5.1"
  }
}
